<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.embeddedsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EmbeddedSystem">
        <activity android:name=".adapter.WifiResultsAdapter"></activity>
        <activity android:name=".adapter.ProjectsListAdapter" />
        <activity android:name=".ui.SearchWifiAccessPointActivity" />
        <activity android:name=".ui.ProjectDetailActivity" />
        <activity android:name=".ui.NewProjectActivity" />
        <activity android:name=".ui.AddOrEditReferencePointActivity" />
        <activity android:name=".ui.AddOrEditAccessPointActivity" />
        <activity android:name=".ui.LocateMeActivity" />
        <activity android:name=".ui.HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>